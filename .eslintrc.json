{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": "off",
        "no-else-return": ["error", {
            "allowElseIf": true
        }],
        "no-console": "off",
        "no-use-before-define": ["error", {
            "functions": false,
            "classes": false
        }]
    }
}
